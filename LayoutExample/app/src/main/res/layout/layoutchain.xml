<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Button
        android:id="@+id/ba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/bb"
        app:layout_constraintHorizontal_chainStyle="spread"
        android:text="A"
        />
    <Button
        android:id="@+id/bb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        app:layout_constraintLeft_toRightOf="@+id/ba"
        app:layout_constraintRight_toLeftOf="@+id/bc"
        app:layout_constraintBaseline_toBaselineOf="@+id/ba"
        />

    <Button
        android:id="@+id/bc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/bb"
        app:layout_constraintBaseline_toBaselineOf="@+id/ba"

        />

    <Button
        android:id="@+id/bd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
app:layout_constraintHorizontal_bias="0.23"
        app:layout_constraintRight_toLeftOf="@+id/be"
        app:layout_constraintTop_toBottomOf="@id/ba" />

    <Button
        android:id="@+id/be"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E"
        app:layout_constraintLeft_toRightOf="@+id/bd"
        app:layout_constraintRight_toLeftOf="@+id/bf"
        app:layout_constraintBaseline_toBaselineOf="@+id/bd"
        />
    <Button
        android:id="@+id/bf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/be"
        app:layout_constraintBaseline_toBaselineOf="@+id/bd"

        />
    <Button
        android:id="@+id/bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf ="@id/be"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/roundedresource"
        app:layout_constraintRight_toLeftOf="@+id/bh"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        android:text="G"
        />
    <Button
        android:id="@+id/bh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="H"
        app:layout_constraintLeft_toRightOf="@+id/bg"
        app:layout_constraintRight_toLeftOf="@+id/bi"
        app:layout_constraintBaseline_toBaselineOf="@+id/bg"
        />
    <Button
        android:id="@+id/bi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/bh"
        app:layout_constraintBaseline_toBaselineOf="@+id/bg"

        />
    <Button
        android:id="@+id/bo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf ="@id/bg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/bp"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        android:text="O"
        />
    <Button
        android:id="@+id/bp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="P"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/bo"
        app:layout_constraintRight_toLeftOf="@+id/bq"
        app:layout_constraintBaseline_toBaselineOf="@+id/bo"
        />
    <Button
        android:id="@+id/bq"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Q"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/bp"
        app:layout_constraintBaseline_toBaselineOf="@+id/bo"

        />


<androidx.cardview.widget.CardView
    android:layout_width="100dp"
    android:layout_height="100dp"
    app:cardCornerRadius="50dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toBottomOf="@id/bo"
    app:layout_constraintBottom_toBottomOf="parent"
    tools:ignore="MissingConstraints">

        <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/aa"
        app:layout_constraintTop_toBottomOf="@id/bo"
        app:layout_constraintLeft_toRightOf="parent" />
</androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>